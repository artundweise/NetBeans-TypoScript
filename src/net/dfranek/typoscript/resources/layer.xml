<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
	<folder name="Editors">
		<folder name="text">
			<folder name="x-typoscript">
				<attr name="SystemFileSystem.localizingBundle" stringvalue="net.dfranek.typoscript.resources.Bundle"/>
                <folder name="FontsColors">
					<folder name="NetBeans">
						<folder name="Defaults">
							<file name="FontAndColors.xml" url="FontAndColors.xml">
								<attr name="SystemFileSystem.localizingBundle" stringvalue="net.dfranek.typoscript.resources.Bundle"/>
                            </file>
						</folder>
					</folder>
				</folder>
				<folder name="BracesMatchers">
					<file name="net-dfranek-typoscript-indent-TSBracesMatcherFactory.instance">
						<attr name="position" intvalue="0"/>
					</file>
				</folder>
				<folder name="FoldManager">
					<file name="org-netbeans-editor-CustomFoldManager$Factory.instance"/>
				</folder>
			</folder>
		</folder>
	</folder>
	<folder name="OptionsDialog">
		<folder name="PreviewExamples">
			<folder name="text">
				<file name="x-typoscript" url="TSExample.ts"/>
			</folder>
		</folder>
	</folder>
	<folder name="Loaders">
		<folder name="text">
			<folder name="x-typoscript">
				<attr name="iconBase" stringvalue="net/dfranek/typoscript/resources/ts_file_16.png"/>
			</folder>
		</folder>
	</folder>
	<folder name="CslPlugins">
		<folder name="text">
			<folder name="x-typoscript">
				<file name="language.instance">
					<attr name="instanceClass" stringvalue="net.dfranek.typoscript.TSLanguage"/>
				</file>
				<file name="structure.instance">
					<attr name="instanceClass" stringvalue="net.dfranek.typoscript.parser.TSStructureScanner"/>
				</file>
			</folder>
		</folder>
	</folder>
</filesystem>
